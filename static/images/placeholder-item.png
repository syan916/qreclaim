<svg width="300" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F8F9FA;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E9ECEF;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="300" height="200" fill="url(#bgGradient)" stroke="#DEE2E6" stroke-width="1"/>
  
  <!-- Icon -->
  <g transform="translate(150, 100)">
    <!-- Box icon -->
    <rect x="-25" y="-20" width="50" height="30" fill="none" stroke="#6C757D" stroke-width="2" rx="3"/>
    <rect x="-20" y="-15" width="40" height="20" fill="none" stroke="#6C757D" stroke-width="1.5" rx="2"/>
    <line x1="-15" y1="-10" x2="15" y2="-10" stroke="#6C757D" stroke-width="1"/>
    <line x1="-10" y1="-5" x2="10" y2="-5" stroke="#6C757D" stroke-width="1"/>
    
    <!-- Question mark -->
    <circle cx="0" cy="15" r="12" fill="#A67B5B" opacity="0.8"/>
    <text x="0" y="22" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white">?</text>
  </g>
  
  <!-- Text -->
  <text x="150" y="160" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#6C757D">No Image Available</text>
</svg>